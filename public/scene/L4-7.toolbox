<xml id="toolbox">
    <block type="variables_set" limit="1">
		<field name="VAR" variabletype=""> x </field>
		<value name="VALUE">
			<shadow type="math_number">
				<field name="NUM">1</field>
			</shadow>
		</value>
	</block>
	<block type="variables_get">
		<field name="VAR" variabletype=""> x </field>
	</block>
	<block type="controls_repeat_ext"></block>
	<block type="math_arithmetic">
		<field name="OP">MULTIPLY</field>
		<value name="A"></value>
        <value name="B">
            <shadow type="math_number">
				<field name="NUM">3</field>
            </shadow>
        </value>
	</block>
	<block type="forward"></block>
	<block type="turn_left"></block>
	<block type="turn_right"></block>
	<block type="jump" limit="3"></block>
</xml>

<xml id="default">
	<block movable="true" deletable="flase" type="when_start" x="20" y="20"><next><block movable="flase" deletable="flase" type="variables_set"><field name="VAR" variabletype=""> x </field><value name="VALUE"><shadow type="math_number"><field name="NUM">1</field></shadow></value></next></block>
</xml>