<xml id="toolbox">
	<block type="forward" limit="2">
		<value name="STEP">
			<shadow type="math_number">
				<field name="NUM">1</field>
			</shadow>
		</value>
	</block>
	<block type="turn_right"></block>
	<block type="jump" limit="5"></block>
</xml>

<xml id="default">
	<block movable="false" deletable="flase" type="when_start" x="20" y="20"><next><block movable="true" type="jump"><next><block movable="true" type="jump"></next></block>
</xml>